SELECT MIN(ARD), * FROM 
	(SELECT max_depth, max_children, att_b, att_c, div_dist, cx, depth, trail, avg(res_depth) as ARD FROM "1_training_no_analysis" 
		GROUP BY max_depth, max_children, att_b, att_c, div_dist, cx, depth)
	GROUP BY depth
UNION
SELECT MIN(ARD), * FROM 
	(SELECT max_depth, max_children, att_b, att_c, div_dist, cx, depth, trail, avg(res_depth) as ARD FROM "5_training_no_analysis" 
		GROUP BY max_depth, max_children, att_b, att_c, div_dist, cx, depth)
	GROUP BY depth
ORDER BY depth